		android:layout_height="wrap_content" android:layout_below="@id/sessiondistanceinflate" />
	<ViewStub android:id="@+id/sessionheartrate"
		android:inflatedId="@+id/sessionheartrateinflate" android:layout="@layout/measurement"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
