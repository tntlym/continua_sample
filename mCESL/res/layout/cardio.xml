	<ViewStub android:id="@+id/action" android:inflatedId="@+id/cardioactioninflate"
		android:layout="@layout/action" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_below="@id/sessionheartrateinflate" />
