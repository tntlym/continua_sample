<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content">
	<Button android:id="@+id/send" android:layout_width="113px"
		android:layout_height="wrap_content" android:text="@string/sendbtn"
		android:gravity="center" android:layout_marginLeft="10dip"
		android:layout_alignParentLeft="true" />
	<Button android:id="@+id/cancel" android:layout_width="113px"
		android:layout_marginLeft="20dip" android:layout_height="wrap_content"
		android:text="@string/cancelbtn" android:gravity="center"


		android:layout_toRightOf="@id/send" android:layout_alignTop="@id/send" />



</RelativeLayout>
