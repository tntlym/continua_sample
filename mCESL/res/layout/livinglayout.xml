<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content">
	<RelativeLayout android:layout_width="480px"
		android:layout_height="wrap_content">
		<RadioGroup android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/tempraturesensor">
			<RadioButton android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="High Temperature"
				android:id="@+id/highTempButton" />
			<RadioButton android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Low Temperature"
				android:id="@+id/lowTempButton" />
			<RadioButton android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="Rate of change too fast"
				android:id="@+id/rateChangeButton" />
		</RadioGroup>
	</RelativeLayout>
	<RelativeLayout android:layout_width="480px"
		android:id="@+id/wtDialogButtonLayout" android:layout_height="wrap_content"
		android:layout_marginTop="225px">
		<include layout="@layout/buttonslayout"></include>
	</RelativeLayout>
</RelativeLayout>
